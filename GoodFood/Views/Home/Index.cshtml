            <div class="col-md-4 col-xs-12">

                <img class="img-responsive Separator" src="@Url.Content("~/images/plate.png")" style="margin-left: -50px;" />

            </div>
            <div class="col-md-8 col-xs-12">
                <label style="font-size: 50px;">SIN GLUTEN. SIN PRESERVANTES. SIN LACTEOS</label><br />
