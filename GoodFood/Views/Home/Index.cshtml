@{
    ViewData["Title"] = "Home Page";
}


<!-- slider start here -->
<div>
    <div style="position: relative;">
        <div id="sliderContainer" class="CenterMainLogo">
            <label class="sliderText1">ESTO NO ES UNA DIETA</label>
            <label class="sliderText2">ESTO ES COMER BIEN</label>
            <button class="CenterMainButton" value="sdasdas">Reservá tu espacio</button>
        </div>
        @*<img id="HeaderLogo" class="img-responsive CenterMainLogo" src="@Url.Content("~/images/white_logo.png")" />*@
    </div>
    <div class="slide">

        <div class="slideshow owl-carousel">
            <div class="item">
                <img src="@Url.Content("~/images/slider1.jpg")" alt="banner" title="banner" class="img-responsive" />
            </div>
            <div class="item">
                <img src="@Url.Content("~/images/slider2.jpg")" alt="banner" title="banner" class="img-responsive" />
            </div>
            <div class="item">
                <img src="@Url.Content("~/images/slider3.jpg")" alt="banner" title="banner" class="img-responsive" />
            </div>
        </div>
    </div>
</div>
<!--  -->
<!--Planes Block -->
<div>
    @*<label class="MainLabelIndicators">Información</label>*@
    <div class="" style="padding:50px;">
        @*<img class="img-responsive Separator" src="@Url.Content("~/images/separadorgf.png")" />*@
        <div class="row">
            <div class="col-md-12 col-xs-12" style="text-align: center; font-size: xx-large;">
                <label>NUESTROS PLANES</label>
            </div>
            <div class="col-md-4" style="background-color: #e58e56; text-align: center; padding: 50px; height: 580px;">
                <label style="font-size: 70px;">21 DIAS</label><br />
                <label>programa completo</label>
                <p style="font-weight: bold;">
                    Limpia tu cuerpo.<br />
                    Elmina la mayor cantidad de toxinas y<br />
                    logra cambiar habitos para estar mejor cada dia.<br />
                    21 dias de comer delicioso para darle un<br />
                    descanso a tu cuerpo y reconectar con tu bienestar.<br /><br />

                    Todastus comidas en la puerta de tu casa u oficina.<br />
                    Charlas cada semana<br />
                    Yoga ilimitado.<br />
                    Descuentos especiales.<br />
                    Y muchos beneficios mas.<br /><br />

                    Cupo limitado
                </p>
                <button class="CenterMainButton" style="width: 300px; margin-bottom: 25px;">Más Info</button>
            </div>
            <div class="col-md-4" style="background-color: #d9ca6b; text-align: center; padding: 50px; height: 580px;">
                <label style="font-size: 70px;">SEMANAS</label><br />
                <label>programa completo</label>
                <p style="font-weight: bold;">
                    Segui comiendo bien todos los dias<br />
                    con nuestros paquetes semanales<br /><br />

                    Desde nuestros combos de batido y sopa,<br />
                    hasta las tres comidas completas a domicilio.<br />
                    De lunes a viernes o la semana completa.<br /><br />

                    No hay excusas para no chinearte.<br />
                    Nosotros te lo hacemos facil.<br />
                </p>
                <button class="CenterMainButton" style="width: 300px; margin-bottom: 25px;">Más Info</button>
            </div>
            <div class="col-md-4" style="background-color: #c195ab; text-align: center; padding: 50px; height: 580px;">
                <label style="font-size: 70px;">SNACKS</label><br />
                <label>programa completo</label>
                <p style="font-weight: bold;">
                    Empeza a hacer los cambios pequeños<br />
                    en tu alimentacion con nuestros snacks<br />
                    o complementa lo que ya estes haciendo<br />
                    con mejores opciones para picar a lo largo del dialog<br /><br />

                    Pedi nuestros paquetes de snacks a domicilio<br />
                    a travez de nuestro menu en linea<br />
                </p>
                <button class="CenterMainButton" style="width: 300px; margin-bottom: 25px;">Más Info</button>
            </div>
        </div>
    </div>
</div>
